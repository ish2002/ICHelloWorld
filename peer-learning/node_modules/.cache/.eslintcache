andleSubmit = this.handleSubmit.bind(this)\n        this.state = {\n            email: '',\n            password: '',\n            result: ' '\n        }\n    }\n\n    // user typed in a field\n    handleChange(event) {\n        this.setState({[event.target.name] : event.target.value})\n    }\n\n    // submit button pressed\n    handleSubmit(event) {\n        event.preventDefault()\n\n        const { email, password } = this.state\n        const shortcode = email.substring(0, email.indexOf('@'))\n\n        // email is the primary key of user database\n        firebase.database().ref('/users/' + shortcode).on('value', (snapshot) => {\n            // checks if there is an account with this email\n            if (snapshot.exists()) {\n                // checks if email and password is correct\n                const mail = snapshot.val().email;\n                const pass = snapshot.val().password;\n                if (password === pass && email === mail) {\n                    // redirects to home page\n                    this.setState({result : \"Login successful!\"})\n                    this.props.history.push(\"/userhome\")\n                } else {\n                    // error message\n                    this.setState({result :  \"Incorrect email or password.\"})\n                }\n            }\n        })\n    }\n\n    render() {\n        return (\n            <form onSubmit = {this.handleSubmit}>\n\n                <h3>Log in</h3>\n                <div className=\"form-group\">\n                    <label>Email</label>\n                    <input name=\"email\" type=\"email\" className=\"form-control\" placeholder=\"Enter email\" value={this.state.email} onChange={this.handleChange} />\n                </div>\n\n                <div className=\"form-group\">\n                    <label>Password</label>\n                    <input name=\"password\" type=\"password\" className=\"form-control\" placeholder=\"Enter password\" value={this.state.password} onChange={this.handleChange} />\n                </div>\n\n                <div className=\"form-group\">\n                    <div className=\"custom-control custom-checkbox\">\n                        <input type=\"checkbox\" className=\"custom-control-input\" id=\"customCheck1\" />\n                        <label className=\"custom-control-label\" htmlFor=\"customCheck1\">Remember me</label>\n                    </div>\n                </div>\n\n                <button type=\"submit\" className=\"btn btn-dark btn-lg btn-block\">\n                    <Link className=\"nav-link\" to=\"/userhome\">Sign in</Link>\n                </button>\n                <p className=\"forgot-password text-right\">\n                    <Link className=\"nav-link\" to=\"/forgotpassword\">Forgot Password</Link>\n                </p>\n\n                <p>{this.state.result}</p>\n            </form>\n        );\n    }\n}","/Users/zeegtab/Documents/Imperial College London 2020 - 2024/Computer Science/ICHELLOWORLD/ICHelloWorld/peer-learning/src/general/signup.js",[],"/Users/zeegtab/Documents/Imperial College London 2020 - 2024/Computer Science/ICHELLOWORLD/ICHelloWorld/peer-learning/src/general/Home.js",["48","49"],"import {Link} from \"react-router-dom\";\nimport React, { Component } from \"react\";\n\nexport default class HomePage extends Component {\n  render() {\n    return (\n      <div>\n        <div className=\"App\">\n      <nav className=\"navbar navbar-expand-lg navbar-light fixed-top\">\n        <div className=\"container\">\n          <Link className=\"navbar-brand\" to={\"/home\"}>IC Peer Learn</Link>\n          <div className=\"collapse navbar-collapse\" id=\"navbarTogglerDemo02\">\n            <ul className=\"navbar-nav ml-auto\">\n              <li className=\"nav-item\">\n                <Link className=\"nav-link\" to={\"/sign-in\"}>Sign in</Link>\n              </li>\n              <li className=\"nav-item\">\n                <Link className=\"nav-link\" to={\"/home\"}>Home</Link>\n              </li>\n              <li className=\"nav-item\">\n                <Link className=\"nav-link\" to={\"/sign-up\"}>Sign up</Link>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </nav>\n\n        <div className=\"outer\">\n          <h1>Welcome To Peer Learning @ Imperial College London!</h1>\n        </div>\n\n        <div className=\"App\">\n              <a className=\"message\">First time? Sign up with your Imperial email address</a>\n              <Link to=\"/sign-up\">\n              <button type=\"Sign up\" className=\"btn btn-dark btn-lg btn-block\">Sign up</button>\n              </Link>\n              <Link to=\"/sign-in\">\n              <a className=\"message\">Already registered? Sign in</a>\n              <button type=\"Sign in\" className=\"btn btn-dark btn-lg btn-block\">Sign in</button>\n              </Link>\n        </div>\n      </div>\n      </div>\n    );\n  }\n}\n","/Users/zeegtab/Documents/Imperial College London 2020 - 2024/Computer Science/ICHELLOWORLD/ICHelloWorld/peer-learning/src/general/groupstudy.js",["50"],"import {Link} from \"react-router-dom\";\nimport React, { Component } from \"react\";\n\nexport default class GroupStudy extends Component {\n    render() {\n        return (\n            <div className=\"Group\">\n              <a>Want to get cracking?</a>\n              <Link to=\"/newstudygroup\">\n              <button type=\"Create my own study group\" className=\"btn btn-dark btn-lg btn-block\">Create my own study group</button>\n              </Link>\n              <Link to=\"/existingstudygroup\">\n              <button type=\"Join my study buddies\" className=\"btn btn-dark btn-lg btn-block\">Join my study buddies</button>\n              </Link>\n            </div>\n        );\n    }\n}",["51","52"],"/Users/zeegtab/Documents/Imperial College London 2020 - 2024/Computer Science/ICHELLOWORLD/ICHelloWorld/peer-learning/src/general/forgotpassword.js",[],"/Users/zeegtab/Docum